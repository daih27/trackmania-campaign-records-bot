{
  "name": "trackmania-campaign-records-bot",
  "version": "1.0.0",
  "description": "Discord bot for tracking and announcing Trackmania player records on campaign maps",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:quiet": "LOG_LEVEL=warn node src/index.js",
    "start:minimal": "LOG_LEVEL=error node src/index.js",
    "start:silent": "LOG_LEVEL=silent node src/index.js",
    "start:debug": "LOG_LEVEL=debug node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:quiet": "LOG_LEVEL=warn nodemon src/index.js"
  },
  "keywords": [
    "discord",
    "bot",
    "trackmania",
    "records",
    "leaderboard"
  ],
  "author": "Daih",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daih27/trackmania-campaign-records-bot.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}